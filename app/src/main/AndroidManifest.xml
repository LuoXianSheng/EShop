<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.newer.eshop" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:theme="@style/OtherAppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".goods.GoodsCarActivity"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".goods.GoodsActivity"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".account.LoginActivity"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".account.RegistActivity"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".classify.ClassifyResultActivity"
            android:theme="@style/AppThemeBlue" />
        <activity
            android:name=".account.SettingActivity"
            android:theme="@style/OtherAppTheme" />
        <!-- QQçš„ -->
        <activity
            android:name="com.tencent.tauth.AuthActivity"
            android:launchMode="singleTask"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tencent1105291740" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tencent.connect.common.AssistActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".me.address.AddressManagerActivity"
            android:theme="@style/OtherAppTheme" >
        </activity>
        <activity
            android:name=".goods.GoodsBuy"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".me.address.MeAddressAddActivity"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".me.address.SelectAddressActivity"
            android:theme="@style/OtherAppTheme" >
        </activity>
        <activity
            android:name=".me.order.AllOrderActivity"
            android:theme="@style/OtherAppTheme" />
        <activity
            android:name=".me.order.SubmitResultActivity"
            android:theme="@style/OtherAppTheme" >
        </activity>
    </application>

</manifest>
